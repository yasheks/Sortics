#include <iostream>

using namespace std;

void generate(int *mas, int n) {
    /*
     * int i=0;
     * while (i<n)
     * {
     *      mas[i]=rand() % 1000;
     *      i++;
     * }
     */
    for (int i = 0; i < n; i++) {
        mas[i] = rand() % 1000;
    }
}

void print(int *mas, int n) {
    for (int i = 0; i < n; i++) {
        cout << mas[i] << " ";
    }
    cout << endl;
}

